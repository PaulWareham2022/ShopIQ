{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "ios": {
        "simulator": false
      }
    },
   "production": {
      "distribution": "store",
      "env": {
        "APP_VARIANT": "production"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "autoIncrement": "buildNumber"
      },
      "env": {
        "NPM_CONFIG_INSTALL_STRATEGY": "hoisted"
      },
      "cache": {
        "key": "production-build-v6"
      },
      "node": "20.11.0"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "paulwareham@me.com",
        "ascAppId": "6752504878",
        "appleTeamId": "766Y9QHTNH"
      }
    }
  }
}
